# Example environment configuration for CampaignHub
# Copy this file to .env and fill in your own values

DATABASE_URL=postgresql://username:password@hostname:5432/database
SESSION_SECRET=changeme
PORT=5000

# AWS SES configuration
SES_HOST=email-smtp.your-region.amazonaws.com
SES_PORT=587
SES_AUTH=login
SES_DOMAIN=example.com
SES_USERNAME=YOUR_AWS_ACCESS_KEY
SES_PASSWORD=YOUR_AWS_SECRET
SES_SENDER=sender@example.com

# Campaign API Configuration
CAMPAIGN_API_URL=https://your-api-endpoint.example.com/campaign

